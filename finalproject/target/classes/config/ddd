<select id="hosname_count" resultType="int">
		select count(*) from hosname
	</select>
	<select id="hosname_list" resultType="dto.hosnameDTO">
		select * from hosname order by
		hosnum
	</select>
	<select id="hosname_insert" parameterType="dto.hosnameDTO">
		insert into hosname
		values(#{hosnum}, #{petkind_kind})
	</select>
	<delete id="hosname_delete" parameterType="map">
		<foreach collection="array" item="item" index="index">
			delete from hosname where hosnum = #{item.hosnum} and #{item.petkind_kind}
		</foreach>
	</delete>
	<select id="hosname_content" parameterType="dto.hosnameDTO"
		resultType="dto.hosnameDTO">
		select * from hosname where hosnum = #{hosnum} and petkind_kind = #{petkind_kind}
	</select>
	<select id="hosname_check" parameterType="dto.hosnameDTO"
		resultType="int">
		select count(*) from hosname where hosnum = #{hosnum} and petkind_kind = #{petkind_kind}
	</select>
	